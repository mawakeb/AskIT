<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="487.0" prefWidth="372.0"
      style="-fx-background-color: #4F5658;" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="nl.tudelft.oopp.demo.controllers.MainSceneController">
    <TabPane prefHeight="487.0" prefWidth="372.0" style="-fx-background-color: #262B2D;" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab closable="false" text="Join Room">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="432.0" prefWidth="372.0"
                                style="-fx-background-color: #4F5658;">
                        <children>
                            <GridPane alignment="CENTER" prefHeight="456.8" prefWidth="372.0"
                                      style="-fx-background-color: #0000;" AnchorPane.bottomAnchor="0.0"
                                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                      AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0"
                                                       percentWidth="15.0" prefWidth="55.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="255.59999999999997" minWidth="10.0"
                                                       prefWidth="253.59999999999997"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0"
                                                       percentWidth="15.0" prefWidth="56.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <children>
                                    <TextField fx:id="link" prefHeight="40.0" prefWidth="256.0"
                                               promptText="Enter link to join a lecture..."
                                               style="-fx-text-fill: #DDDFDF; -fx-background-color: #262B2D;"
                                               GridPane.columnIndex="1" GridPane.rowIndex="2">
                                        <cursor>
                                            <Cursor fx:constant="TEXT"/>
                                        </cursor>
                                    </TextField>
                                    <Button fx:id="join" mnemonicParsing="false" onAction="#joinButtonClicked"
                                            prefHeight="32.0" prefWidth="60.0" style="-fx-background-color: C4D1D3;"
                                            text="Join" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                                            GridPane.rowIndex="3" GridPane.valignment="CENTER">
                                        <GridPane.margin>
                                            <Insets/>
                                        </GridPane.margin>
                                    </Button>
                                    <TextField editable="false"
                                               style="-fx-background-color: #0000; -fx-padding: 0,0,0,0; -fx-text-fill: #DDDFDF; -fx-font-size: 210%;"
                                               text="Join Lecture Room" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                        <font>
                                            <Font name="System Bold" size="24.0"/>
                                        </font>
                                    </TextField>
                                </children>
                            </GridPane>
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab closable="false" text="Create Room">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                style="-fx-background-color: #4F5658;">
                        <children>
                            <GridPane prefHeight="456.8" prefWidth="372.0" AnchorPane.bottomAnchor="0.0"
                                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                      AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0"
                                                       percentWidth="15.0" prefWidth="55.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0"
                                                       percentWidth="15.0" prefWidth="55.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" percentHeight="30.0" prefHeight="30.0"
                                                    vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <children>
                                    <ListView fx:id="list" onMouseClicked="#copyList" prefHeight="174.0"
                                              prefWidth="290.0" style="-fx-background-color: #0000;"
                                              GridPane.columnIndex="1" GridPane.rowIndex="5">
                                        <cursor>
                                            <Cursor fx:constant="DEFAULT"/>
                                        </cursor>
                                    </ListView>
                                    <TextField fx:id="name" prefHeight="40.0" prefWidth="260.0"
                                               promptText="Enter lecture name..."
                                               style="-fx-text-fill: #DDDFDF; -fx-background-color: #262B2D;"
                                               GridPane.columnIndex="1" GridPane.rowIndex="2">
                                        <cursor>
                                            <Cursor fx:constant="TEXT"/>
                                        </cursor>
                                    </TextField>
                                    <Button fx:id="create" mnemonicParsing="false" onAction="#createButtonClicked"
                                            prefHeight="32.0" prefWidth="60.0" style="-fx-background-color: C4D1D3;"
                                            text="Create" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                                            GridPane.rowIndex="3" GridPane.valignment="CENTER"/>
                                    <TextField alignment="TOP_LEFT" editable="false"
                                               style="-fx-background-color: #0000; -fx-padding: 0,0,0,0; -fx-text-fill: #DDDFDF; -fx-font-size: 210%;"
                                               text="Create Lecture Room" GridPane.columnIndex="1"
                                               GridPane.rowIndex="1">
                                        <font>
                                            <Font name="System Bold" size="24.0"/>
                                        </font>
                                    </TextField>
                                </children>
                            </GridPane>
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
        </tabs>
    </TabPane>
</VBox>
